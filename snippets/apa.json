    "body": "makefont ${1:fntname} name \"${2:Tahoma}\" pointsize ${3:size} def;\n$4",
    "description": "Code snippet for makefont ... def"
  },
  "makefont":{
    "prefix": "makefont",
    "body": "makefont ${1:fntname} name \"${2:Tahoma}\" pointsize ${3:size};\n$4",
    "description": "Code snippet for makefont"
  },
  "paper":{
    "prefix": "paper",
    "body": "paper \"${1:name}\" manufacturer \"${2:name}\" weight ${3:80};\n$4",
    "description": "Code snippet for paper instruction"
  },
  "file":{
    "prefix": "file",
    "body": "file \"${1:filepath}\" fix ${2:length};\n$3",
    "description": "Code snippet for file instruction"
  },
  "box":{
    "prefix": "box",
    "body": "box ${1:xsize} ${2:ysize} at ${3:x} ${4:y} lead ${5:pels} ${6:down|back|up};\n$7",
    "description": "Code snippet for box instruction"
  },
  "page":{
    "prefix": "page",
    "body": "page ${1:xsize} ${2:inch|cm|mm|pels|point} ${3:ysize} ${4:inch|cm|mm|pels|point};\n$5",
    "description": "Code snippet for page instruction"
  }
}